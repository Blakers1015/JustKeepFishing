<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Gallary">

    <Gallery
        android:id="@+id/gallery"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="138dp"
        android:layout_height="152dp"
        android:layout_below="@+id/imageView3"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-167dp"
        android:layout_toEndOf="@+id/imageView2"
        android:adjustViewBounds="true"
        android:src="@drawable/fish1" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="122dp"
        android:layout_height="133dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="119dp"
        android:layout_marginTop="1dp"
        android:adjustViewBounds="true"
        android:src="@drawable/fish2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="120dp"
        android:layout_height="167dp"
        android:adjustViewBounds="true"
        android:src="@drawable/fish3" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="126dp"
        android:layout_height="127dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="151dp"
        android:adjustViewBounds="true"
        android:src="@drawable/fish4" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="118dp"
        android:layout_height="120dp"
        android:layout_alignTop="@+id/imageView4"
        android:layout_marginStart="12dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/imageView3"
        android:adjustViewBounds="true"
        android:src="@drawable/fish5" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="124dp"
        android:layout_height="125dp"
        android:layout_alignTop="@+id/imageView4"
        android:layout_marginStart="5dp"
        android:layout_marginTop="-8dp"
        android:layout_toEndOf="@+id/imageView5"
        android:adjustViewBounds="true"
        android:src="@drawable/fish6" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="101dp"
        android:layout_height="103dp"
        android:layout_below="@+id/imageView4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="11dp"
        android:adjustViewBounds="true"
        android:src="@drawable/fish7" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="101dp"
        android:layout_height="103dp"
        android:layout_below="@+id/imageView4"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="4dp"
        android:layout_toStartOf="@+id/imageView1"
        android:adjustViewBounds="true"
        android:src="@drawable/fish8" />

</RelativeLayout>